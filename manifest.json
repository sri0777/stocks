{
  "manifest_version": 3,
  "name": "Wealth Creator",
  "description": "Select Your Stocks",
  "version": "1.0",
  "action": {
    "default_popup": "public/views/login.html"
  },
  "permissions": ["activeTab", "storage"],
  "web_accessible_resources": [
    {
      "resources": ["views/login.html", "views/style.css", "script.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; img-src 'self';"
  }
}
